# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Database Configuration
SUPABASE_DB_URL=postgresql://postgres:password@localhost:54322/postgres
DATABASE_URL=your_database_connection_string

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# ML Configuration
ML_MODEL_BASE_PATH=/models
ENABLE_GPU=false
ML_CONFIDENCE_THRESHOLD=0.3
MAX_ADAPTATIONS_PER_SESSION=10

# Edge Runtime Configuration
EDGE_CONFIG=your_edge_config_url

# Analytics & Monitoring
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id
ENABLE_ANALYTICS=true
LOG_LEVEL=info

# Security
CORS_ORIGINS=*
RATE_LIMIT_REQUESTS_PER_MINUTE=100

# Feature Flags
ENABLE_REAL_TIME=true
ENABLE_ML_INFERENCE=true
ENABLE_PROGRESSIVE_DISCLOSURE=true
ENABLE_ERROR_PREVENTION=true

# Development Configuration
NODE_ENV=development
NEXT_PUBLIC_VERCEL_ENV=development
NEXT_PUBLIC_DEBUG_MODE=false
SUPABASE_LOCAL_URL=http://localhost:54321
SUPABASE_LOCAL_ANON_KEY=your_local_anon_key